<ul>
    <li><span class="label">{% trans %}Branches{% endtrans %}:</span></li>
    {% for branch in repository.branches %}
        <li>
            <a
                class="icon branch{{ branch == active_branch ? ' active' : '' }}"
                href="{{ path('repository_branch', { 'ref': branch.name }) }}">{{ branch.name }}</a>
        </li>
    {% endfor %}
    <li>
        <a href="{{ path('branch_new') }}" class="icon new">new branch</a>
    </li>
</ul>