{% extends "CypressGitElephantBundle::base.html.twig" %}

{% block title %}
    {{ tree.path }}{{ tree.blob is not null ? '/' ~ tree.blob : '' }} at {{ parent() }}
{% endblock %}

{% block navigation %}
    {% include "CypressGitElephantBundle:Branch:_branch_list.html.twig" %}
    {% include "CypressGitElephantBundle:Repository:_tag_list.html.twig" %}
{% endblock %}

{% block content %}
    {{ parent() }}
    {% include "CypressGitElephantBundle:Repository:_breadcrumb.html.twig" %}
    {% include "CypressGitElephantBundle:Repository:_tree.html.twig" with {
        'tree':          tree,
        'baseRoute':     'repository_root',
        'subtreeRoute':  'repository_tree'
    } %}
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script type="text/javascript" src="{{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shCore.js') }}"></script>
    <script src="{{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shAutoloader.js') }}" type="text/javascript"></script>

    <script type="text/javascript">
    SyntaxHighlighter.autoloader(
        'applescript            {{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shBrushAppleScript.js') }}',
        'actionscript3 as3      {{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shBrushAS3.js') }}',
        'bash shell             {{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shBrushBash.js') }}',
        'coldfusion cf          {{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shBrushColdFusion.js') }}',
        'cpp c                  {{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shBrushCpp.js') }}',
        'c# c-sharp csharp      {{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shBrushCSharp.js') }}',
        'css                    {{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shBrushCss.js') }}',
        'delphi pascal          {{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shBrushDelphi.js') }}',
        'diff patch pas         {{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shBrushDiff.js') }}',
        'erl erlang             {{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shBrushErlang.js') }}',
        'groovy                 {{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shBrushGroovy.js') }}',
        'java                   {{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shBrushJava.js') }}',
        'jfx javafx             {{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shBrushJavaFX.js') }}',
        'js jscript javascript  {{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shBrushJScript.js') }}',
        'perl pl                {{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shBrushPerl.js') }}',
        'php                    {{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shBrushPhp.js') }}',
        'text plain             {{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shBrushPlain.js') }}',
        'py python              {{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shBrushPython.js') }}',
        'ruby rails ror rb      {{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shBrushRuby.js') }}',
        'sass scss              {{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shBrushSass.js') }}',
        'scala                  {{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shBrushScala.js') }}',
        'sql                    {{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shBrushSql.js') }}',
        'vb vbnet               {{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shBrushVb.js') }}',
        'xml xhtml xslt html    {{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/scripts/shBrushXml.js') }}'
    );
    SyntaxHighlighter.all();
    </script>
{% endblock %}
{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" media="screen, projection" type="text/css" href="{{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/styles/shCore.css') }}" />
    <link rel="stylesheet" media="screen, projection" type="text/css" href="{{ asset('bundles/cypressgitelephant/js/syntaxhighlighter/styles/shThemeDefault.css') }}" />
{% endblock %}


